.table-desktop{
   tr{
     td{
       font-size: 16px;
       color: #6A737D;
       padding: 10px 15px;
       border-color: #eaeaea!important;
     }
    td:nth-child(1){
      padding-left: 0;
    }
    td:nth-child(2){
      white-space: normal;
      word-wrap: break-word;
      word-break: break-all;
    }
  }
}

.table-mobile{
   tr{
     td{
       font-size: 12px;
       color: #6A737D;
       padding: 10px 0;
       border-color: #eaeaea!important;
     }
    td:nth-child(1){
      border-color: #eaeaea;
    }
    td:nth-child(2){
      text-align: right;
      white-space: normal;
      word-wrap: break-word;
      word-break: break-all;
    }
  }
}