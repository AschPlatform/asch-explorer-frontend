.table-top-border{
  .q-table-top{
    border-bottom: 1px dashed #000000;
    padding-left: 0;
    margin-bottom: 20px;
    .q-table-title{
      font-size: 14px;
      font-weight: 600;
      color: #333333;
    }
  }
  tr{
    th{
      font-size: 14px;
      color: #333333;
    }
  }
}

.table-tr-td-p-0{
   tr{
    td:nth-child(1){
      font-size: 14px;
      color: #333333;
      padding-left: 0;
    }
  }
}

.custom-breadcrumbs-a{
  a:hover{
    color: #0033ff!important;
  }
  a{
    color:#000000!important;
    font-size: 18px;
    font-weight: 400;
    cursor: pointer;
  }
}

.custom-border-td{
  padding 0
  span{
    width 100%
    height 100%
    padding 4px 12px
    border:1px solid #cccccc!important;
    border-radius: 20px !important;
  }
}


.custom-pre-wrap{
  width: 60%;
  // font-size: 10px;
  white-space: pre-wrap; /*css-3*/ 
  white-space: -moz-pre-wrap; /*Mozilla,since1999*/ 
  white-space: -pre-wrap; /*Opera4-6*/ 
  white-space: -o-pre-wrap; /*Opera7*/ 
  word-wrap: break-word; /*InternetExplorer5.5+*/ 
}

