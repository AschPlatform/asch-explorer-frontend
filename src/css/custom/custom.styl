.custorm-search-icon:hover{
  i.q-icon{
    color:#2D98FC!important;
  }
}

.custorm-search-icon{
  .q-input-target, .q-input-shadow{
    font-size: 12px;
    color: #ffffff;
  }
  i.q-icon{
    widows 24px;
    height: 24px;
    color: #ACACAC!important;
    margin-right: 0px;
    align-self: auto;
    display: inline-block!important;
  }
}

.custorm-select{
  height: 29px;
  padding-left: 19px;
  padding-right: 13px; 
  .q-if-inner{
    .q-input-target, .q-input-shadow{
    color:#9DA0B5;
    font-size: 14px;
    }
  }
  i.q-if-control{
    color: #9DA0B5!important;
  }
}

.q-popover{
  background: #000000!important;
  .q-list{
    padding: 0;
    border:1px solid rgba(157,160,181,1);
    border-radius:4px;
    .q-item{
      background: #000000!important;
      color: #9DA0B5;
      text-align: center;
    }
    .q-select-highlight{
      color: #2D98FC;
      background: #000000;
    }
  }
}

.custom-breadcrumbs-a{
  a:hover{
    color: #2D98FC!important;
  }
  a{
    color:#6A737D!important;
    font-size: 16px;
    font-weight: 400;
    cursor: pointer;
  }
}

