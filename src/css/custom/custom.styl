.custorm-search-icon:hover{
  i.q-icon{
    color:#2D98FC!important;
  }
}

.custorm-search-icon{

  .q-input-target, .q-input-shadow{
    font-size: 12px;
    color: #ffffff;
  }
  i.q-icon{
    widows 24px;
    height: 24px;
    color: #ACACAC!important;
    margin-right: 0px;
    align-self: auto;
    display: inline-block!important;
  }
}

.custorm-select{
  height: 29px;
  padding-left: 19px;
  padding-right: 13px; 
  .q-if-inner{
    .q-input-target, .q-input-shadow{
    color:#9DA0B5;
    font-size: 14px;
    }
  }
  i.q-if-control{
    color: #9DA0B5;
  }
}

.custom-breadcrumbs-a{
  a:hover{
    color: #2B89E3!important;
  }
  a{
    color:#6A737D!important;
    font-size: 16px;
    font-weight: 400;
    cursor: pointer;
  }
}

.panelitem-container{
  position: relative;
  .right-icon{
    position: absolute;
    right: 0;
    bottom: 0;
  }
}