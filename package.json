    "vue-i18n": "^7.3.3",
    "vue-qr": "^1.3.8"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
